{
    "widgets": [
        {
            "name": "Auto Selection",
            "type": "auto",
            "coordinates": [0, 3],
            "should-title": false,
            "autos": [
                "RIGHT_ROCKET"
            ]
        },
        {
            "name": "Encouragement!",
            "type": "encouragement",
            "coordinates": [0, 0],
            "should-title": false
        },
        {
            "name": "Link to Data Stream",
            "type": "link",
            "source": "\\data.html",
            "coordinates": [3, 3],
            "should-title": false
        },
        {
            "name": "Elevator Calibrated",
            "type": "boolean",
            "condition": "source",
            "source": ["superstructure_status", "elevatorIsCalibrated"],
            "coordinates": [0, 1],
            "should-title": true,
            "colors": {
              "if_true": "#00ff00",
              "if_false": "#ff0000"
            }
        },
        {
            "name": "Wrist Calibrated",
            "type": "boolean",
            "condition": "source",
            "source": ["superstructure_status", "wristIsCalibrated"],
            "coordinates": [0,2],
            "should-title": true,
            "colors": {
              "if_true": "#00ff00",
              "if_false": "#ff0000"
            }
        },
        {
            "name": "Drivetrain Left Encoder > .01",
            "type": "boolean",
            "source": ["drivetrain_input", "leftEncoder"],
            "condition": "Math.abs(source) > .01",
            "coordinates": [2, 1],
            "should-title": true,
            "colors": {
              "if_true": "#00ff00",
              "if_false": "#ff0000"
            }
        },
        {
            "name": "Drivetrain Right Encoder > .01",
            "type": "boolean",
            "source": ["drivetrain_input", "rightEncoder"],
            "condition": "Math.abs(source) > .01",
            "coordinates": [2, 0],
            "should-title": true,
            "colors": {
              "if_true": "#00ff00",
              "if_false": "#ff0000"
            }
        },
        {
            "name": "Drivetrain Left Encoder Not 0",
            "type": "boolean",
            "source": ["drivetrain_input", "leftEncoder"],
            "condition": "source != 0",
            "coordinates": [2, 2],
            "should-title": true,
            "colors": {
              "if_true": "#00ff00",
              "if_false": "#ff0000"
            }
        },
        {
            "name": "Drivetrain Right Encoder Not 0",
            "type": "boolean",
            "source": ["drivetrain_input", "rightEncoder"],
            "condition": "source != 0",
            "coordinates": [2, 3],
            "should-title": true,
            "colors": {
              "if_true": "#00ff00",
              "if_false": "#ff0000"
            }
        }
    ],
    "settings": {
        "size": [4, 4]
    }
}
